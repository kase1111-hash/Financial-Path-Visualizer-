{
  "name": "financial-path-visualizer",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "A tool that shows you where your money decisions lead",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "idb": "^8.0.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.17.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  }
}
